{
  "uid": "services-overview",
  "title": "Services Overview",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Active Users (Total)",
      "id": 1,
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "targets": [
        { "expr": "sum(max_over_time(app_active_users_total[5m]))", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none", "decimals": 0, "min": 0 }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false }, "justifyMode": "center" }
    },
    {
      "type": "stat",
      "title": "Requests/sec (Total)",
      "id": 2,
      "datasource": "Prometheus",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
      "targets": [
        { "expr": "sum(rate(http_request_duration_seconds_count[1m]))", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s", "decimals": 2, "min": 0 } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false }, "justifyMode": "center" }
    },
    {
      "type": "timeseries",
      "title": "Requests/sec by Service (job)",
      "id": 3,
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 6, "w": 16, "h": 8 },
      "targets": [
        { "expr": "sum by (job)(rate(http_request_duration_seconds_count[1m]))", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s", "decimals": 2, "min": 0 } }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx) by Service",
      "id": 4,
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 14, "w": 16, "h": 8 },
      "targets": [
        { "expr": "sum by (job)(rate(http_request_duration_seconds_count{code=~\"5..\"}[5m]))", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s", "decimals": 2, "min": 0 } }
    }
  ],
  "timezone": "browser",
  "links": []
}
