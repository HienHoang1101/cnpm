{
  "id": null,
  "uid": "order-service-dashboard",
  "title": "Order Service - Metrics",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Order Service Traffic (Req/s)",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {"expr": "rate(http_request_duration_seconds_count{job=\"order-service\"}[1m])", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Average Response Time (seconds)",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {"expr": "rate(http_request_duration_seconds_sum{job=\"order-service\"}[1m]) / rate(http_request_duration_seconds_count{job=\"order-service\"}[1m])", "refId": "A"}
      ]
    }
  ],
  "timezone": "browser"
}
