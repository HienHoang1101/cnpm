    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "targets": [
        {"expr": "sum by (job) (rate(http_request_duration_ms_count{job=~\".*payment.*\"}[1m]))", "legendFormat": "{{job}}", "refId": "A"}
      ],
      "title": "Payment Request Rate (req/s)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "targets": [
        {"expr": "sum by (job) (rate(process_cpu_user_seconds_total{job=~\".*payment.*\"}[1m]) + rate(process_cpu_system_seconds_total{job=~\".*payment.*\"}[1m]))", "legendFormat": "{{job}}", "refId": "A"}
      ],
      "title": "Payment Process CPU (cores)",
      "type": "timeseries"
